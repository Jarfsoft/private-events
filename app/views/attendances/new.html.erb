<h2>Create a new attendance</h2>
<%= form_for @attendance do |f| %>

  <p>
    <%= f.label :attendee_id %> <br/>
    <%= f.text_field :attendee_id, value: current_user.id %>
  </p>
  <p>
    <%= f.label :attended_event_id %><br />
    <%= f.text_area :attended_event_id, value: @attendance.attended_event_id  %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>

